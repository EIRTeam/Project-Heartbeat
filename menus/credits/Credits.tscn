script/source = "extends Panel

const SCROLL_SPEED = 50
const LEAD_IN_TIME = 3
var lead_in_t = 0.0

onready var container = get_node(\"Control/VBoxContainer\")

func _ready():
	container.rect_position += Vector2(0, rect_size.y)

func _process(delta):
	container.rect_position -= Vector2(0, SCROLL_SPEED*delta)
	if container.rect_position.y <= -(container.rect_size.y):
		container.rect_position = Vector2(0, rect_size.y)
"

